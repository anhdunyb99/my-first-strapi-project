{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "minLength": 1
    },
    "content": {
      "type": "text",
      "required": true,
      "minLength": 1,
      "maxLength": 5000
    },
    "users_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "posts"
    },
    "post_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post-category.post-category",
      "mappedBy": "post_id"
    }
  }
}
